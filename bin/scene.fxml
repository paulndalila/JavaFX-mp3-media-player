<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="196.0" prefWidth="724.0" style="-fx-background-color: #222;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <Label fx:id="songLabel" alignment="CENTER" layoutX="18.0" layoutY="-1.0" prefHeight="118.0" prefWidth="694.0" text="MP3 Player" textFill="#0de835">
         <font>
            <Font name="Segoe Script" size="23.0" />
         </font>
      </Label>
      <ProgressBar fx:id="songProgressBar" layoutX="4.0" layoutY="116.0" prefHeight="18.0" prefWidth="713.0" progress="0.0" />
      <Button fx:id="playButton" layoutY="135.0" mnemonicParsing="false" onAction="#playMedia" prefHeight="55.0" prefWidth="71.0" text="PLAY">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="pauseButton" layoutX="71.0" layoutY="134.0" mnemonicParsing="false" onAction="#pauseMedia" prefHeight="55.0" prefWidth="82.0" text="PAUSE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="resetButton" layoutX="154.0" layoutY="134.0" mnemonicParsing="false" onAction="#resetMedia" prefHeight="55.0" prefWidth="89.0" text="RESET">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="previousButton" layoutX="243.0" layoutY="134.0" mnemonicParsing="false" onAction="#previousMedia" prefHeight="55.0" prefWidth="109.0" text="PREVIOUS">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="nextButton" layoutX="352.0" layoutY="134.0" mnemonicParsing="false" onAction="#nextMedia" prefHeight="55.0" prefWidth="71.0" text="NEXT">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ComboBox fx:id="speedBox" layoutX="423.0" layoutY="134.0" onAction="#changeSpeed" prefHeight="55.0" prefWidth="82.0" promptText="SPEED" />
      <Slider fx:id="volumeSlider" layoutX="505.0" layoutY="155.0" max="200.0" prefHeight="14.0" prefWidth="219.0" value="50.0" />
   </children>
</AnchorPane>
